var xhr = new XMLHttpRequest();

// Configura a requisição - método GET para uma URL de exemplo
xhr.open('GET', 'https://ea3baf39-e1e9-4737-9c28-8a869b79c933-00-1spc09uux8b1t.riker.repl.co/', true);

// Define o que fazer quando a requisição estiver concluída
xhr.onload = function() {
  if (xhr.status >= 200 && xhr.status < 300) {
var l = false
document.addeventlistener("keydown", function(e) {
if (e.key === 'j') {
l = true
}
}
    while (l) {
        var k = new KeyboardEvent('keydown', { key: 'e', keyCode: 69, code: 'KeyE' });
        window.dispatchEvent(k);
        await mss(1);
        var kp = new KeyboardEvent('keyup', { key: 'e', keyCode: 69, code: 'KeyE' });
        window.dispatchEvent(kp);
    }

function mss(ms) {
    return new Promise(resolve => setTimeout(resolve, ms));
}
    console.log('Resposta:', xhr.responseText);
  } else {
    // Houve um erro na requisição
    console.error('Erro na requisição:', xhr.statusText);
  }
};

// Define o que fazer em caso de erro na requisição
xhr.onerror = function() {
  console.error('Erro de conexão.');
};

// Envia a requisição
xhr.send();
