var xhr = new XMLHttpRequest();

xhr.open('GET', 'https://ea3baf39-e1e9-4737-9c28-8a869b79c933-00-1spc09uux8b1t.riker.repl.co/', true);

xhr.onload = function() {
  if (xhr.status >= 200 && xhr.status < 300) {
const url = 'https://sao-paulo-0-alpaca.dynast.cloud:8443/';

// Dados que você quer enviar (pode ser um objeto JavaScript)
const dados = {
  nome: 'João',
  idade: 30,
  email: 'joao@email.com'
};

// Opções para a requisição, incluindo o método POST e os dados a serem enviados
const options = {
  method: 'POST',
  headers: {
    'Content-Type': 'application/json'
  },
  body: JSON.stringify(dados)
};

// Enviar a requisição usando fetch
fetch(url, options)
  .then(response => {
    if (!response.ok) {
      throw new Error('Erro ao enviar os dados');
    }
    return response.json();
  })
  .then(data => {
    console.log('Dados enviados com sucesso:', data);
  })
  .catch(error => {
    console.error('Erro:', error);
  });
    console.log('connected');
  } else {
    console.error('fetch error', xhr.statusText);
  }
};

xhr.onerror = function() {
  alert('cant fetch');
};

xhr.send();
